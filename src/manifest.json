{

  "manifest_version": 2,
  "name": "Redirect GoogleJP To Jisho",
  "version": "1.0",

  "description": "Redirects Google.co.jp searches to Jisho.org to help with learning Japanese from ebooks.",
  "homepage_url": "https://github.com/sheodox/redirect-googlejp-to-jisho",

  "permissions": [
    "*://*.google.co.jp/search*",
    "*://google.co.jp/search*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}